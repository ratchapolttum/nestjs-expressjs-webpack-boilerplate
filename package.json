{
  "version": "1.0.0",
  "name": "nestjs-expressjs-webpack-boilerplate",
  "description": "Boilerplate for NestJS (Express.js) Webpack",
  "author": "ratchapolttum",
  "repository": {
    "type": "git",
    "url": "https://github.com/ratchapolttum/nestjs-expressjs-webpack-boilerplate.git"
  },
  "bugs": "https://github.com/ratchapolttum/nestjs-expressjs-webpack-boilerplate/issues",
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.{cjs,js,json,json5,jsonc,jsonl,md,mjs,ts,yaml,yml}",
    "lint:fix": "eslint --fix **/*.{cjs,js,json,json5,jsonc,jsonl,md,mjs,ts,yaml,yml}",
    "build": "nest build",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit-watch": "jest --watch",
    "test:unit-coverage": "jest --coverage",
    "test:e2e": "jest --config ./e2e/jest-e2e.config.ts",
    "test:e2e-watch": "jest --config ./e2e/jest-e2e.config.js --watch"
  },
  "dependencies": {
    "@nestjs/common": "11.0.7",
    "@nestjs/core": "11.0.7",
    "@nestjs/platform-express": "11.0.7",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.19.0",
    "@eslint/markdown": "6.2.2",
    "@nestjs/cli": "11.0.2",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.7",
    "@suites/di.nestjs": "3.0.1",
    "@suites/doubles.jest": "3.0.1",
    "@suites/unit": "3.0.1",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.16",
    "@types/supertest": "6.0.2",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-jsonc": "2.19.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-sonarjs": "3.0.1",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-yml": "1.16.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "jsonc-eslint-parser": "2.4.0",
    "prettier": "3.4.2",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.23.0",
    "yaml-eslint-parser": "1.2.3"
  },
  "private": true,
  "engines": {
    "node": "^20"
  }
}

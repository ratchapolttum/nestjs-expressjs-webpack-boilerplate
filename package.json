{
  "version": "1.0.0",
  "name": "nestjs-expressjs-webpack-boilerplate",
  "description": "Boilerplate for NestJS (Express.js) Webpack",
  "author": "ratchapolttum",
  "repository": {
    "type": "git",
    "url": "https://github.com/ratchapolttum/nestjs-expressjs-webpack-boilerplate.git"
  },
  "bugs": "https://github.com/ratchapolttum/nestjs-expressjs-webpack-boilerplate/issues",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit-watch": "jest --watch",
    "test:unit-coverage": "jest --coverage",
    "test:e2e": "jest --config ./e2e/jest-e2e.config.ts",
    "test:e2e-watch": "jest --config ./e2e/jest-e2e.config.js --watch"
  },
  "dependencies": {
    "@nestjs/common": "11.0.7",
    "@nestjs/core": "11.0.7",
    "@nestjs/platform-express": "11.0.7",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.2",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.7",
    "@suites/di.nestjs": "3.0.1",
    "@suites/doubles.jest": "3.0.1",
    "@suites/unit": "3.0.1",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.16",
    "@types/supertest": "6.0.2",
    "jest": "29.7.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3"
  },
  "private": true,
  "engines": {
    "node": "^20"
  }
}
